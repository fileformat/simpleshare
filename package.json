{
  "name": "simpleshare.io",
  "version": "0.1.0",
  "description": "Support 'share' links for static sites without invasive tracking",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "deploy": "now --public -e NODE_ENV=production -e GA_ID=$GA_ID --token $NOW_TOKEN --npm",
    "alias": "now alias --token=$NOW_TOKEN"
  },
  "dependencies": {
    "express": "^4.15.4",
    "handlebars": "^4.0.12",
    "koa": "^2.5.3",
    "koa-pino-logger": "^2.1.3",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "request": "^2.83.0"
  },
  "engines": {
    "node": "8.1.4"
  },
  "repository": {
    "url": "https://www.github.com/fileformat/simpleshare"
  },
  "keywords": [
    "sharing",
    "social",
    "redirect",
    "bookmark"
  ],
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/koa": "^2.0.46",
    "@types/koa-pino-logger": "^2.1.1",
    "@types/koa-router": "^7.0.32",
    "@types/koa-static": "^4.0.0",
    "@types/koa-views": "^2.0.3",
    "@types/node": "^10.11.4",
    "@types/request": "^2.47.1",
    "pino-pretty": "^2.2.1",
    "typescript": "^3.1.1"
  }
}
